#include <stdio.h>

// Function to calculate gcd using Euclid's algorithm
int gcd(int a, int b) {
    while (b != 0) {
        int temp = b;
        b = a % b;
        a = temp;
    }
    return a;
}

int main() {
    for (int a = 0; a < 26; a++) {
        if (gcd(a, 26) == 1) {
            printf("%d is allowed (invertible mod 26)\n", a);
        } else {
            printf("%d is NOT allowed\n", a);
        }
    }
    return 0;
}
